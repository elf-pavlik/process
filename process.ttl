@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vf: <https://w3id.org/valueflows/> .

# Classes

vf:Agent
  a owl:Class ;
  rdfs:label "Agent" ;
  rdfs:comment "" .

vf:Process
  a owl:Class ;
  rdfs:label "Process" ;
  rdfs:comment "" ;

vf:Resource
  a owl:Class ;
  rdfs:label "Resource" ;
  rdfs:comment "" ;
  
# Properties

vf:inflow
  a rdf:Property ;
  rdfs:label "inflow" ;
  rdfs:range rdf:Property .

vf:consume
  a rdf:Property ;
  rdfs:label "consume" ;
  owl:subPropertyOf vf:inflow .
  
vf:use
  a rdf:Property ;
  rdfs:label "use" ;
  owl:subPropertyOf vf:inflow .
  
vf:name
  a skos:prefLabel ;
  rdfs:label "name" ;
  rdfs:range skos:prefLabel ;
  rdfs:domain vf:Process .

vf:context
  a owl:ObjectProperty ;
  rdfs:label "context" ;
  rdfs:domain vf:Process ;
  rdfs:range vf:Agent .
  
vf:note
  a owl:DatatypeProperty ;
  rdfs:label "note" ;
  rdfs:domain vf:Process, vf:Resource .
